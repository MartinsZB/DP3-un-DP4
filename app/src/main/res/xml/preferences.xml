<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/setting_gr_skana">
    <ListPreference
        android:dialogTitle="@string/setting_skana_komandas"
        android:entries="@array/skanas_nosaukumi"
        android:entryValues="@array/skanas_id"
        android:key="command_sound_string"
        android:summary="%s"
        android:title="@string/setting_skana_komandas"
        android:defaultValue="balss"/>
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="minute_beep_boolean"
        android:title="@string/settings_skana_minute"
        android:summary="@string/settings_skana_minuteDescription"/>
    </PreferenceCategory>
</PreferenceScreen>